{
  "order": 0,
  "index_patterns": [
    "conductor_*_message*"
  ],
  "settings": {
    "index": {
      "refresh_interval": "1s",
      "unassigned": {
        "node_left": {
          "delayed_timeout": "5m"
        }
      },
      "number_of_shards": "1",
      "number_of_replicas": "1"
    }
  },
  "mappings": {
    "properties": {
      "payload": {
        "index": true,
        "type": "keyword"
      },
      "created": {
        "type": "long"
      },
      "messageId": {
        "index": true,
        "type": "keyword"
      },
      "queue": {
        "index": true,
        "type": "keyword"
      }
    }
  },
  "aliases": {
    "MESSAGES": {}
  }
}
